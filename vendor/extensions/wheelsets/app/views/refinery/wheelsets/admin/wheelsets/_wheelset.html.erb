<li class='clearfix record <%= cycle("on", "on-hover") %>' id="<%= dom_id(wheelset) -%>">
  <span class='title'>
    <%= wheelset.name %>

  </span>
  <span class='actions'>
    
    <%= link_to refinery_icon_tag("application_go.png"), refinery.wheelsets_wheelset_path(wheelset),
        :title => t('.view_live_html'),
        :target => "_blank" %>
    
    <%= link_to refinery_icon_tag("application_edit.png"), refinery.edit_wheelsets_admin_wheelset_path(wheelset),
         :title => t('.edit') %>
    <%= link_to refinery_icon_tag("delete.png"), refinery.wheelsets_admin_wheelset_path(wheelset),
        :class => "cancel confirm-delete",
        :title => t('.delete'),
        :confirm => t('message', :scope => 'refinery.admin.delete', :title => wheelset.name),
        :method => :delete %>
  </span>
</li>
